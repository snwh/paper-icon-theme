[Icon Theme]
_Name=Paper
_Comment=A simple and modern icon theme.
Inherits=Adwaita,gnome,hicolor
Example=folder

# KDE Specific Stuff
DisplayDepth=32
LinkOverlay=link_overlay
LockOverlay=lock_overlay
